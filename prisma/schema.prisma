// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  relationMode = "prisma"
}

model Apotek{
  id String @id @default(cuid())
  lat Float
  long Float
  name String
  detail String
}

model NamaPoli{
  id String @id @default(cuid())
  namapoli String
}

model JadwalPoli{
  id String @id @default(cuid())
  tanggal DateTime
  idpoli String
}