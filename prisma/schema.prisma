// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  relationMode = "prisma"
}

model Apotek{
  id String @id @default(cuid())
  lat Float
  long Float
  name String
  detail String
}

model NamaPoli{
  id String @id @default(cuid())
  namapoli String
  jadwals Jadwal[]
}


model Jadwal{
  id String @id @default(cuid())
  idpoli String
  idjadwalpoli String
  namaPoli     NamaPoli     @relation(fields: [idpoli], references: [id])
  jadwalpoli   JadwalPoli   @relation(fields: [idjadwalpoli], references: [id])
}



model JadwalPoli{
  id String @id @default(cuid())
  hari String
  waktu String
  jadwals Jadwal[]
}



